# Configuration pour le projet LinkedIn Prise de Contact
# Copiez ce fichier vers .env et remplissez les valeurs

# OpenAI API Key (requis pour CrewAI)
OPENAI_API_KEY=your_openai_api_key_here

# Horizon Data Wave (HDW) Configuration
# Token d'accès pour les outils HDW
HDW_ACCESS_TOKEN=your_hdw_access_token_here

# ID du compte HDW
HDW_ACCOUNT_ID=your_hdw_account_id_here

# Configuration optionnelle
# Langue par défaut (fr pour français, en pour anglais)
DEFAULT_LANGUAGE=fr

# Niveau de verbosité (True/False)
VERBOSE=True
